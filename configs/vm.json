{
  "object_type": "vm_startup",
  "range_low": 1,
  "range_high": 2,
  "qemu_binary": "qemu-system-x86_64",
  "max_ram": "128m",
  "base_name": "VM-test",
  "misc_params": " -machine type=pc,accel=kvm",
  "kernel_image": {
    "dir": "/home/costash/linux_3.14_built",
    "image_name": "linux",
    "init_params": {
      "init": "../scripts/vm_init.sh",
      "console": "ttyS0,115200n8",
      "root": "/dev/root",
      "rootflags": {
        "trans": "virtio",
        "version": "9p2000.u"
      },
      "mode": "ro",
      "rootfstype": "9p"
    }
  },
  "properties": [
    {
      "dev": "chardev",
      "type": "serial",
      "id": "mgmt",
      "role": "guest_management",
      "socket_name": "mgmt.socket"
    },
    {
      "dev": "chardev",
      "type": "mon",
      "id": "monitor",
      "role": "qemu_monitor",
      "socket_name": "mon.socket"
    },
    {
      "dev": "fsdev",
      "type": "device",
      "id": "fsdev-root",
      "path": "/",
      "mount_tag": "/dev/root",
      "device_id": "fs-root",
      "device_type": "virtio-9p-pci"
    },
    {
      "dev": "fsdev",
      "type": "device",
      "id": "fsdev-home",
      "path": "/home/costash/workspace/vm_homes/vmrootfs",
      "mount_tag": "overlayshare",
      "device_id": "fs-home",
      "device_type": "virtio-9p-pci"
    },
    {
      "dev": "fsdev",
      "type": "device",
      "id": "fsdev-kernel",
      "path": "/home/costash/linux_3.14_built",
      "mount_tag": "kernelshare",
      "device_id": "fs-kernel",
      "device_type": "virtio-9p-pci"
    },
    {
      "dev": "netdev",
      "type": "tap",
      "id": "management0",
      "device_type": "virtio-net-pci"
    },
    {
      "dev": "netdev",
      "type": "tap",
      "id":   "net",
      "device_type": "virtio-net-pci"
    }
  ]
}
